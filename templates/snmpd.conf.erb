rocommunity		<%= @snmp[:rocommunity] %>

sysDescr		<%= @snmp[:sysDescr] %>
sysLocation		<%= @snmp[:sysLocation] %>
sysContact		<%= @snmp[:sysContact] %>
trapsink		<%= @snmp[:trapsink] %>
linkUpDownNotifications	<%= @snmp[:linkUpDownNotifications] %>
master			<%= @snmp[:master] %>

<% for extend in @snmp[:extends] do -%>
exec			<%= extend %>
<% end -%>
